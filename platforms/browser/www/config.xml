<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.bikeshare" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>BikeShare</name>
    <description>
        A simple Cordova app for Toronto BikeShare
    </description>
    <author email="jessica@sjostrom.ca">
        Jessica Sjostrom
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
